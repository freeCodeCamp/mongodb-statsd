{
  "name": "mongodb-statsd",
  "version": "0.0.1",
  "description": "mongodb metrics to graphite application",
  "main": "app.js",
  "dependencies": {
    "debug": "^3.1.0",
    "lodash": "^4.17.4",
    "mongodb": "^1.3.23",
    "rxjs": "^5.4.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "commitizen": "^2.9.6",
    "cz-freecodecamp": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "^0.14.3",
    "prettier": "^1.7.4",
    "validate-commit-msg": "^2.14.0"
  },
  "scripts": {
    "lint": "eslint --ext=js,jsx .",
    "lint:fix": "eslint . --ext=js,jsx --fix",
